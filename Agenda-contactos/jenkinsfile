pipeline{
   agent any
   
    stages{
        stage("install"){
            steps{
                git branch: 'main', url: 'https://github.com/Omar2108/micro-servicio-agenda-contacto.git'
            }
        }
        stage('test'){
            steps{
                 dir('Agenda-contactos'){
                    sh 'mvn test'
                 }
            }
        }
    }
}
